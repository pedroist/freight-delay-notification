    "@mui/material": "^6.4.8",
    "@react-google-maps/api": "^2.20.6",
    "@sendgrid/mail": "^8.1.4",
    "@tanstack/react-query": "^5.69.0",
    "@temporalio/activity": "^1.11.7",
    "@temporalio/client": "^1.11.7",
    "@temporalio/worker": "^1.11.7",
    "@temporalio/workflow": "^1.11.7",
    "axios": "^1.8.4",
    "next": "15.2.3",
    "openai": "^4.89.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/google.maps": "^3.58.1",
